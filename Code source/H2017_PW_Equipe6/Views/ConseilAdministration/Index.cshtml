@model IEnumerable<H2017_PW_Equipe6.Models.ConseilAdministration>

@{
    ViewBag.Title = "Conseil d'administration - " + Session["nameClub"];
}



<div class="container">
    <h2>Notre conseil d'administration</h2>

    @foreach (var membre in Model)
    {
        <div class="col-md-4 col-sm-6">
            <div class="panel panel-default">
                <div class="panel-heading">
                    <h3>@membre.prenomCA @membre.nomCA</h3>
                </div>
                <div class="panel-body">
                    <b>@membre.REF_RoleCA.descriptionROLECA</b>
                </div>
            </div>
        </div>
    }

</div>