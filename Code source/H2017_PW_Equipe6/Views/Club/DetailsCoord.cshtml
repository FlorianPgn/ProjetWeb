@model H2017_PW_Equipe6.Models.Club

@{
    ViewBag.Title = "Coordonnées du club";
    ViewBag.Type = "Coordonnées";
}

<nav class="navbar navbar-default navbar-static-top">
    <div class="container-fluid">
        <div class="navbar-header">
            <a class="navbar-brand" href="#">@ViewBag.Title</a>
        </div>
        <ul class="nav navbar-nav">
            <li class="active"><a href="#">Afficher</a></li>
            <li>@Html.ActionLink("Modifier", "EditCoord", "Club")</li>
        </ul>
    </div>
</nav>

<div class="container">
    <h2>Affichage</h2>
    <h4>@ViewBag.Type</h4>
	<hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.nom)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.nom)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.adresse)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.adresse)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.ville)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.ville)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.province)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.province)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.codePostal)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.codePostal)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.numTelephone)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.numTelephone)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.courriel)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.courriel)
        </dd>
    </dl>
    <hr />
    <p>
        @Html.ActionLink("Editer", "EditCoord") |
        @Html.ActionLink("Retour à l'affichage", "DetailsCoord")
    </p>
</div>
